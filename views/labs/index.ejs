<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>All Labs</h1>
  <ul>
    <% labs.forEach(function(l) { %>
      <li>
        <%= l.lab %>
          - 
        <%= l.done ? 'done' : 'not done' %>
      </li>
    <% }); %>
  </ul>
</body>
</html>